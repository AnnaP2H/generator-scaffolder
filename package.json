{
  "name": "generator-p2h",
  "version": "0.0.0",
  "description": "scaffolding tool for projects",
  "homepage": "https://www.psd2html.com/",
  "author": {
    "name": "P2H",
    "email": "test@p2h.com",
    "url": "https://www.psd2html.com/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "gulp",
    "webpack",
    "boilerplate",
    "P2H",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "babel-eslint": "^8.2.6",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-undef": 1,
      "no-unused-vars": 1,
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-floating-decimal": 2,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "ImportDeclaration": true,
            "VariableDeclarator": true
          }
        }
      ],
      "no-multi-str": 2,
      "camelcase": 1,
      "eol-last": 1,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-before-blocks": 2,
      "space-before-function-paren": [
        2,
        "never"
      ],
      "arrow-spacing": 2,
      "prettier/prettier": "error"
    },
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "repository": "mrlss/generator-p2h",
  "license": "MIT"
}
